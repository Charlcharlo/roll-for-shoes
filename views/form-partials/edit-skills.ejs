<% character.skills.forEach(element => { %>
    <div class="inputSkillAndDice">
        <input class="r4sInput" type="text" name="skillName[]" placeholder="Skill Name" value="<%= element.name %>" required>
        <div class="formDieContainer">
            <input class="r4sInput" id="editDieNum<%= element.cc %>" type="number" value="<%= element.dice %>" name="skillDice[]" required>
            <h2 class="subTitle">d6</h2>
            <button class="deleteBtn deleteSkill">
                <span class="material-symbols-outlined">
                    delete
                </span>
            </button>
        </div>
    </div>
<% }); %>
<button class="contentBtn" type="button" id="addSkillBuilder">
    Add Skill
    <span class="material-symbols-outlined">
        add_circle
    </span>
</button>